{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2017",
		"noImplicitAny": false,
		"sourceMap": false,
		"outDir": "build",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"es2017",
			"esnext.asynciterable"
		],
		"moduleResolution": "node"
	},
	"exclude": [
		"node_modules"
	],
	"include": ["src/**/*.ts"]
}
